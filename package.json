{
  "name": "finbot",
  "version": "1.0.0",
  "description": "Bot Telegram para controle financeiro pessoal com IA",
  "main": "dist/app.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "db:migrate": "ts-node src/database/migrate.ts",
    "frontend:install": "cd frontend && npm install",
    "frontend:build": "cd frontend && npm run build",
    "setup": "npm install && npm run frontend:install && npm run frontend:build && npm run db:migrate",
    "all": "npm run frontend:build && npm run dev"
  },
  "keywords": ["telegram", "bot", "finance", "openai"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "mysql2": "^3.6.5",
    "node-telegram-bot-api": "^0.64.0",
    "openai": "^4.24.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/node-telegram-bot-api": "^0.64.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
